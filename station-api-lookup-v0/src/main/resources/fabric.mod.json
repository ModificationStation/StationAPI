{
  "schemaVersion": 1,
  "id": "station-api-lookup-v0",
  "version": "${version}",

  "name": "Station API Lookup (v0)",
  "description": "Ability to dynamically extract APIs from BlockEntities and Items, inspired by Fabric.",
  "authors": [
    "Modification Station"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/repo/mod/stationapi",
    "sources": "https://github.com/ModificationStation/StationAPI",
    "issues": "https://github.com/ModificationStation/StationAPI/issues"
  },

  "license": "MIT",
  "icon": "assets/station-api-lookup-v0/icon.png",

  "environment": "client",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.modificationstation.stationapi.impl.lookup.SelfLookupImpl"
    ]
  },
  "depends": {
    "fabricloader": "*",
    "minecraft": "1.0.0-beta.7.3"
  },

  "custom": {
    "modmenu:api": true
  }
}